@model IEnumerable<TestToka.Data.Entities.Auto>
@{
    ViewBag.Title = "Index";
}
<p>
    @Html.ActionLink("Añadir nuevo auto", "Create")
</p>
<table class="table">
    <tr>


        <th>
            @Html.DisplayNameFor(model => model.Marca)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Modelo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Color)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TipoTransmision)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DescripcionEstatica)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Folio)
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
    <tr>

        <td>
            @Html.DisplayFor(modelItem => item.Marca)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Modelo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Color)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TipoTransmision)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DescripcionEstatica)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Folio)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
            @Html.ActionLink("Details", "Details", new { id = item.Id }) 
        </td>
    </tr>
        
    }
</table>
